     1.0
   ]
  },
  {
   "qname": "params.diffuse",
   "required": false,
   "containerType": "Array",
   "cardinality": [
    3,
    3
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "params.diffuse.*",
   "required": false,
   "valueType": "number",
   "valueSchema": "Float",
   "valueRange": [
     0.0,
     1.0
   ]
  },
  {
   "qname": "params.specular",
   "required": false,
   "containerType": "Array",
   "cardinality": [
    3,
    3
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "params.specular.*",
   "required": false,
   "valueType": "number",
   "valueSchema": "Float",
   "valueRange": [
     0.0,
     1.0
   ]
  },
  {
   "qname": "params.shininess",
   "required": false,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Float"
  },
  {
