{
  "title": "Value",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Value for attributeByteCount, attributeValues and objectIds.",
  "properties": {
    "valueType": {
      "type": "string",
      "description": "Defines the value type.",
      "enum": [
        "Int8",
        "UInt8",
        "Int16",
        "UInt16",
        "Int32",
        "UInt32",
        "Float32",
        "Float64",
        "String"
      ]
    },
    "property": {
      "type": "string",
      "description": "Encoding method for the value.",
      "enum": [
        "count",
        "attributeValuesByteCount"
      ],
      "enum-description": {
        "count": "Should always be present and indicates the count of features in the attribute storage."
      }
    }
  },
  "required": [
    "valueType",
    "property"
  ],
  "additionalProperties": false,
  "esriDocumentation": {}
}