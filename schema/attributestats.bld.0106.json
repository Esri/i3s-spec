{
  "title": "attributeStatistics [building profile]",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Concatenated attribute statistics. If needed, the type of the attribute (string or number) may be inferred from `mostFrequentValues` and/or `min`/`max` fields.",
  "properties": {
    "fieldName": {
      "type": "string",
      "description": "Name of the field."
    },
    "label": {
      "type": "string",
      "description": "Label of the field name. If label is empty, the label and fieldName are identical."
    },
    "modelName": {
      "type": "string",
      "enum": [
        "category",
        "family",
        "familyType",
        "bldgLevel",
        "createdPhase",
        "demolishedPhase",
        "discipline",
        "assemblyCode",
        "omniClass",
        "systemClassifications",
        "systemType",
        "systemName",
        "systemClass",
        "custom"
      ],
      "description": "A fixed string of building information, similar to a filter. Used by client applications to define specific behavior for the modelName. The [default filter types](./defaultFilterTypes.bld.md) define the modelName for the attribute statistics."
    },
    "min": {
      "type": "number",
      "description": "Minimum value. Numeric attributes only."
    },
    "max": {
      "type": "number",
      "description": "Maximum value. Numeric attributes only."
    },
    "mostFrequentValues": {
      "type": "array",
      "minItems": 0,
      "maxItems": 256,
      "items": {
        "type": [
          "integer",
          "string"
        ]
      },
      "description": "Most frequent value, if applicable for this attribute. Truncated to 256 entries."
    },
    "subLayerIds": {
      "type": "array",
      "items": {
        "type": "integer"
      },
      "description": "List of sublayers where this attribute may be found."
    }
  },
  "required": [
    "fieldName",
    "subLayerIds"
  ],
  "additionalProperties": false,
  "esriDocumentation": {
    "examples": [
      {
        "title": "`String` attribute with `modelName` to specify filterable semantic",
        "code": {
          "fieldName": "fournitures",
          "label": "Meuble interieur",
          "modelName": "custom",
          "mostFrequentValues": [
            "chair",
            "table",
            "cubicle",
            "boxes"
          ],
          "subLayerIds": [
            10,
            4,
            5
          ]
        }
      },
      {
        "title": "`Integral` attribute with `modelName` to specify filterable semantic ",
        "code": {
          "fieldName": "floor",
          "label": "Etages",
          "modelName": "bldgLevel",
          "mostFrequentValues": [
            1,
            2,
            3
          ],
          "subLayerIds": [
            10,
            15
          ]
        }
      },
      {
        "title": "`Float` attribute",
        "code": {
          "fieldName": "diameter",
          "label": "Diametre de conduit",
          "min": 0.2566,
          "max": 2.256,
          "subLayerIds": [
            3,
            5
          ]
        }
      }
    ]
  }
}