{
  "title": "fullExtent",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Defines the full extent of a scene layer.",
  "properties": {

    "spatialReference": {
      "type": "object",
      "description": "An object containing the WKID or WKT identifying the spatial reference of the layer's geometry.",
      "$ref": "spatialReference.cmn.0106.json"
    },
    "xmin": {
      "type": "number",
      "description": "left"
    },
    "xmax": {
      "type": "number",
      "description": "right"
    },
    "ymin": {
      "type": "number",
      "description": "bottom"
    },
    "ymax": {
      "type": "number",
      "description": "top"
    },
    "zmin": {
      "type": "number",
      "description": "lowest elevation"
    },
    "zmax": {
      "type": "number",
      "description": "highest elevation"
    }
  },
  "required": [
    "xmin",
    "ymin",
    "zmin",
    "xmax",
    "ymax",
    "zmax"
  ],
  "additionalProperties": false,
  "esriDocumentation": {
    "examples": [
      {
        "title": "Building Scene Layer",
        "code": {
          "xmin": -117.855689264791,
          "ymin": 32.5702577626442,
          "xmax": -116.87086222794,
          "ymax": 34.1460567673275,
          "zmin": 0.1,
          "zmax": 50.0,
          "spatialReference": {
            "wkid": 4326,
            "latestWkid": 4326
          }
        }
      }

    ]
  }
}
