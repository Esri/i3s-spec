{
  "title": "featureAttribute",
  "type": "object",
  "$schema": "http://json-schema.org/draft-04/schema",
  "description": "Declaration of the attributes per feature in the geometry, such as feature ID or face range.",
  "properties": {
    "id": {
        "type": "object",
        "description": "ID of the feature attribute.",
        "$ref": "value_schema.json"
    },
    "faceRange": {
        "type": "object",
        "description": "Describes the face range of the feature attribute.",
        "$ref": "value_schema.json"
      }
  },
  "additionalProperties": false,
  "esriDocumentation": {
    "examples": [
       {
         "title": "featureAttribute example",
         "code": {
          "id": {
            "valueType": "UInt64",
            "valuesPerElement": 1
        },
        "faceRange": {
            "valueType": "UInt32",
            "valuesPerElement": 2
        }
           }
        }   
      ]
   }
}
