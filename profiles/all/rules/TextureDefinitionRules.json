{
  "targetProfile": "all",
  "resource": "TextureDefinition",  
  "version": "1.4",
  "rules": [
  {
   "qname": "encoding",
   "required": true,
   "containerType": "Array",
   "cardinality": [
    1,
    4
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "encoding.*",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None",
   "valueDomain": [
    "image/png", "image/jpg", "image/jpeg", "image/vnd-ms.dds"
   ]
  },
  {
   "qname": "wrap",
   "required": true,
   "containerType": "Array",
   "cardinality": [
    1,
    2
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "wrap.*",
   "required": true,
   "valueType": "string",
   "valueSchema": "None",
   "valueDomain": [
    "none", "repeat", "mirror"
   ]
  },
  {
   "qname": "atlas",
   "required": true,
   "containerType": "None",
   "valueType": "boolean",
   "valueSchema": "None"
  },
  {
   "qname": "uvSet",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None",
   "valueDomain": [
    "uv0", "uv1", "uv2", "uv3","uv4", "uv5", "uv6", "uv7"
   ]
  },
  {
   "qname": "channels",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None",
   "valueDomain": [
    "rgb", "rgba", "h", "n", "d", "a"
   ]
  },
  {
   "qname": "regions",
   "required": false,
   "containerType": "Array",
   "cardinality": [
    1,
    4096
   ],
   "valueType": "object",
   "valueSchema": "None",
   "valueDependencies": [ {
    "to": "../atlas",
    "type": "IS",
    "value": true
   } ]
  },
  {
   "qname": "regions.*",
   "required": true,
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "regions.*.id",
   "required": true,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Integer",
   "valueRange": [
    0,
    4095
   ]
  },
  {
   "qname": "regions.*.subimageRegion",
   "required": true,
   "containerType": "Array",
   "cardinality": [
    4,
    4
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "regions.*.subimageRegion.*",
   "required": true,
   "valueType": "number",
   "valueSchema": "Float",
   "valueRange": [
    0.0,
    1.0
   ]
  },
  {
   "qname": "images",
   "required": true,
   "containerType": "Array",
   "cardinality": [
    1,
    -1
   ],
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "images.*",
   "required": true,
   "valueType": "object",
   "valueSchema": "None"
  },
  {
   "qname": "images.*.id",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None"
  },
  {
   "qname": "images.*.size",
   "required": true,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Integer",
   "valueRange": [
    0,
    8192
   ]
  },
  {
   "qname": "images.*.pixelInWorldUnits",
   "required": false,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "None",
   "valueRange": [
    0,
    1.7976931348623157e+308
   ]
  },
  {
   "qname": "images.*.href",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "URL"
  },
  {
   "qname": "images.*.byteOffset",
   "required": false,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Integer",
   "valueRange": [
    0,
    1.7976931348623157e+308
   ]
  },
  {
   "qname": "images.*.length",
   "required": false,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Integer",
   "valueRange": [
    0,
    1.7976931348623157e+308
   ]
  }
  ]
}