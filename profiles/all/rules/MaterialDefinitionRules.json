    "to": "../../type",
    "type": "IS",
    "value": "water"
   } ]
  },
  {
   "qname": "params.waveAmplitude",
   "required": true,
   "containerType": "None",
   "valueType": "number",
   "valueSchema": "Float",
   "valueRange": [
    1,
    255
   ],
   "valueDependencies": [ {
    "to": "../../type",
    "type": "IS",
    "value": "water"
   } ]
  } ]
 }