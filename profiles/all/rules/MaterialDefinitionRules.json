   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None",
   "valueDependencies": [ {
    "to": "../type",
    "type": "IS",
    "value": "reference"
   } ]
  },
  {
   "qname": "name",
   "required": false,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None"
  },
  {
   "qname": "type",
   "required": true,
   "containerType": "None",
   "valueType": "string",
   "valueSchema": "None",
   "valueDomain": [
    "standard", "water", "billboard", "leafcard", "reference"
   ]
  },
  {
   "qname": "params",
   "required": true,
   "containerType": "None",
   "valueType": "object",
   "valueSchema": "None",
   "valueDependencies": [ {
    "to": "../type",
    "type": "NOT",
    "value": "reference"
   } ]
  },
  {
